const { Router } = require("express");
const DoThis=require("../controller/functions")
const router=Router()



router.get("/signup",DoThis.cookieJWTAuth,DoThis.singup)
router.post("/signup",DoThis.cookieJWTAuth,DoThis.singup)

router.get(`/VerificationEmail`,DoThis.cookieJWTAuth,DoThis.tokenval)

router.get("/login",DoThis.cookieJWTAuth,DoThis.login)
router.post("/login",DoThis.cookieJWTAuth,DoThis.login)














module.exports=router